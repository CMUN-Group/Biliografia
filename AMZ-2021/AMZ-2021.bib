   publisher = {Paraninfo},
   title = {Instrumentación electrónica},
   url = {https://books.google.com.co/books?id=Fb5tBQAAQBAJ},
   year = {2014},
}
@book{areny2005sensores,
   abstract = {Libro dirigido a estudiantes y profesionales de la ingeniería electrónica, su objetivo es enseñar el fundamento de los sensores y el diseño de los circuitos de acondicionamiento de señal asociados. Los sensores están agrupados según la magnitud eléctrica que varía (resistencia, inductancia, capacidad) o que se genera. Incluye un capítulo orientado a los sensores digitales y otro a sensores inteligentes e instrumentación digital, contaemplando también las interfaces directas sensor-microcontrolador y otro a los sensores en uniones p-n, MOSFET, CCD, ultrasonidos, fibras ópticas y biosensores. En un primer capítulo se introduce la terminología, los fundamentos de los sensores, los materiales en que se basan y las técnicas de fabricación de microsensores. Se incluyen tanto los sensores clásicos (galgas, RTD, termistores, LVDT, sincros, termopares, piezoeléctricos) como los microsensores (piezorresistivos, efecto Hall, efecto Wiegand, autorresonantes, de óxido metálico). En cada capítulo hay problemas propuestos y ejemplos resueltos, y en el apéndice final las soluciones a los problemas planteados.},
   author = {R P Areny},
