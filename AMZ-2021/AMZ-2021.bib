@article{Kamminga2018,
   abstract = {© 2018 by the authors. Licensee MDPI, Basel, Switzerland. Between 1960 and 1990, 95% of the black rhino population in the world was killed. In South Africa, a rhino was killed every 8 h for its horn throughout 2016. Wild animals, rhinos and elephants, in particular, are facing an ever increasing poaching crisis. In this paper, we review poaching detection technologies that aim to save endangered species from extinction. We present requirements for effective poacher detection and identify research challenges through the survey. We describe poaching detection technologies in four domains: perimeter based, ground based, aerial based, and animal tagging based technologies. Moreover, we discuss the different types of sensor technologies that are used in intruder detection systems such as: radar, magnetic, acoustic, optic, infrared and thermal, radio frequency, motion, seismic, chemical, and animal sentinels. The ultimate long-term solution for the poaching crisis is to remove the drivers of demand by educating people in demanding countries and raising awareness of the poaching crisis. Until prevention of poaching takes effect, there will be a continuous urgent need for new (combined) approaches that take up the research challenges and provide better protection against poaching in wildlife areas.},
   author = {Jacob Kamminga and Eyuel Ayele and Nirvana Meratnia and Paul Havinga},
   doi = {10.3390/s18051474},
   issn = {14248220},
   issue = {5},
   journal = {Sensors (Switzerland)},
   keywords = {Anti-poaching,Conservation,Intruder detetection,Surveillance},
   title = {Poaching detection technologies-A survey},
   volume = {18},
   year = {2018},
}
@article{GobiernodeColombia2018,
   abstract = {El Gobierno expidió el Decreto 734 el pasado 13 de abril y con ello definió el nuevo reglamento del Estatuto General de Contratación Pública. Según el Ejecutivo, resultaba indispensable incluir en una sola norma las reglas para adelantar los procesos contractuales, los contratos y demás asuntos relacionados. La disposición regula temas como la planeación contractual, la publicidad y las garantías en la contratación, las modalidades de selección, los mecanismos de enajenación de bienes del Estado, la protección de la industria nacional, el Registro Único de Proponentes y la contratación pública electrónica, entre otros. De esta forma, se reglamentan las leyes 80 de 1993, 361 de 1997, 816 del 2003, 905 del 2004, 996 del 2005, 1150 del 2007, 1450 y 1474 del 2011 y el decreto-ley 019 del 2012. Además, se derogan expresamente 17 decretos, como el 2170 del 2002, 1170 del 2008, 2474 del 2008, 4828 del 2008, 4444 del 2008, 2025 del 2009, 1430 del 2010, 1464 del 2010, 2473 del 2010 y 2516 del 2011,entre otros.},
   author = {Gobierno de Colombia},
   journal = {Departamento Nacional de Planeación},
   title = {Plan Nacional de Desarrollo 2018-2022. Pacto por Colombia. Pacto por la Equidad},
   year = {2018},
}
